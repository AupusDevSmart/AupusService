# Template de Variáveis de Ambiente
# Copie este arquivo para .env e preencha os valores

# API URL - Mude conforme o ambiente
# Desenvolvimento: http://localhost:3000/api/v1
# Produção: https://aupus-service-api.aupusenergia.com.br/api/v1
VITE_API_URL="http://localhost:3000/api/v1"
VITE_APP_NAME="AupusService"

# PUSHER
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=
PUSHER_SCHEME=
PUSHER_APP_CLUSTER=

VITE_PUSHER_APP_ID="${PUSHER_APP_ID}"
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_APP_SECRET="${PUSHER_APP_SECRET}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

VITE_SENTRY_DSN=
VITE_WEB_VITALS=false
VITE_STANDALONE=false

VITE_DEFAULT_DOMAIN="http://localhost:5173"

VITE_S3_URL="https://aupusdev.s3.amazonaws.com"
